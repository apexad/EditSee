<?xml version="1.0" encoding="UTF-8" ?>
<form>
	<field>
		<name>existing password</name>
		<type>text</type>
	</field>
	<field>
		<name>new password</name>
		<type>text</type>
	</field>
	<field>
		<name>re-type new password</name>
		<type>text</type>
	</field>
	<button>
		<name>submit</name>
		<function>xajax_changePassword</function>
	</button>
</form>
<popup>
	<title>Change Password</title>
	<height>140px</height>
</popup>